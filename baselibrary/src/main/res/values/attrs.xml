<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="base_ShoppingView">
        <attr name="base_sv_bg_color" format="color" />
        <attr name="base_sv_text" format="string" />
        <attr name="base_sv_text_size" format="dimension" />
        <attr name="base_sv_duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="base_MsgView">
        <!-- 圆角矩形背景色 -->
        <attr name="base_mv_backgroundColor" format="color" />
        <!-- 圆角弧度,单位dp-->
        <attr name="base_mv_cornerRadius" format="dimension" />
        <!-- 圆角弧度,单位dp-->
        <attr name="base_mv_strokeWidth" format="dimension" />
        <!-- 圆角边框颜色-->
        <attr name="base_mv_strokeColor" format="color" />
        <!-- 圆角弧度是高度一半-->
        <attr name="base_mv_isRadiusHalfHeight" format="boolean" />
        <!-- 圆角矩形宽高相等,取较宽高中大值-->
        <attr name="base_mv_isWidthHeightEqual" format="boolean" />
    </declare-styleable>

    <declare-styleable name="base_TGlideTabLayout">
        <!-- indicator -->
        <attr name="base_indicator_color" />
        <attr name="base_indicator_height" />
        <attr name="base_indicator_width" />
        <attr name="base_indicator_margin_left" />
        <attr name="base_indicator_margin_top" />
        <attr name="base_indicator_margin_right" />
        <attr name="base_indicator_margin_bottom" />
        <attr name="base_indicator_corner_radius" />
        <attr name="base_indicator_gravity" />
        <attr name="base_indicator_style" />
        <attr name="base_indicator_width_equal_title" />

        <!-- underline -->
        <attr name="base_underline_color" />
        <attr name="base_underline_height" />
        <attr name="base_underline_gravity" />

        <!-- divider -->
        <attr name="base_divider_color" />
        <attr name="base_divider_width" />
        <attr name="base_divider_padding" />

        <!-- tab -->
        <attr name="base_tab_padding" />
        <attr name="base_tab_space_equal" />
        <attr name="base_tab_width" />

        <!-- title -->
        <attr name="base_titlesize" />
        <attr name="base_textSelectColor" />
        <attr name="base_textUnSelectColor" />
        <attr name="base_textBold" />
        <attr name="base_textAllCaps" />

    </declare-styleable>

    <!-- **************************** TabLayout 自定义View 属性 start**************************** -->
    <!-- indicator -->
    <!-- 设置显示器颜色 -->
    <attr name="base_indicator_color" format="color" />
    <!-- 设置显示器高度 -->
    <attr name="base_indicator_height" format="dimension" />
    <!-- 设置显示器固定宽度 -->
    <attr name="base_indicator_width" format="dimension" />
    <!-- 设置显示器margin,当indicator_width大于0,无效 -->
    <attr name="base_indicator_margin_left" format="dimension" />
    <attr name="base_indicator_margin_top" format="dimension" />
    <attr name="base_indicator_margin_right" format="dimension" />
    <attr name="base_indicator_margin_bottom" format="dimension" />
    <!-- 设置显示器圆角弧度-->
    <attr name="base_indicator_corner_radius" format="dimension" />
    <!-- 设置显示器上方还是下方,只对圆角矩形有用-->
    <attr name="base_indicator_gravity" format="enum">
        <enum name="TOP" value="48" />
        <enum name="BOTTOM" value="80" />
    </attr>
    <!-- 设置显示器为常规|三角形|背景色块|-->
    <attr name="base_indicator_style" format="enum">
        <enum name="NORMAL" value="0" />
        <enum name="TRIANGLE" value="1" />
        <enum name="BLOCK" value="2" />
    </attr>
    <!-- 设置显示器长度与title一样长,只有在STYLE_NORMAL并且indicatorWidth小于零有效-->
    <attr name="base_indicator_width_equal_title" format="boolean" />
    <!-- 设置显示器支持动画-->
    <attr name="base_indicator_anim_enable" format="boolean" />
    <!-- 设置显示器动画时间-->
    <attr name="base_indicator_anim_duration" format="integer" />
    <!-- 设置显示器支持动画回弹效果-->
    <attr name="base_indicator_bounce_enable" format="boolean" />

    <!-- underline -->
    <!-- 设置下划线颜色 -->
    <attr name="base_underline_color" format="color" />
    <!-- 设置下划线高度 -->
    <attr name="base_underline_height" format="dimension" />
    <!-- 设置下划线上方还是下方-->
    <attr name="base_underline_gravity" format="enum">
        <enum name="TOP" value="48" />
        <enum name="BOTTOM" value="80" />
    </attr>

    <!-- divider -->
    <!-- 设置分割线颜色 -->
    <attr name="base_divider_color" format="color" />
    <!-- 设置分割线宽度 -->
    <attr name="base_divider_width" format="dimension" />
    <!-- 设置分割线的paddingTop和paddingBottom -->
    <attr name="base_divider_padding" format="dimension" />

    <!-- tab -->
    <!-- 设置tab的paddingLeft和paddingRight -->
    <attr name="base_tab_padding" format="dimension" />
    <!-- 设置tab大小等分 -->
    <attr name="base_tab_space_equal" format="boolean" />
    <!-- 设置tab固定大小 -->
    <attr name="base_tab_width" format="dimension" />

    <!-- title -->
    <!-- 设置字体大小 -->
    <attr name="base_titlesize" format="dimension" />
    <!-- 设置字体选中颜色 -->
    <attr name="base_textSelectColor" format="color" />
    <!-- 设置字体未选中颜色 -->
    <attr name="base_textUnSelectColor" format="color" />
    <!-- 设置字体加粗 -->
    <attr name="base_textBold" format="enum">
        <enum name="NONE" value="0" />
        <enum name="SELECT" value="1" />
        <enum name="BOTH" value="2" />
    </attr>
    <!-- 设置字体全大写 -->
    <attr name="base_textAllCaps" format="boolean" />
    <!-- **************************** TabLayout 自定义View 属性 end **************************** -->

    <declare-styleable name="base_TSectionTabLayout">
        <!-- indicator -->
        <attr name="base_indicator_color" />
        <attr name="base_indicator_height" />
        <attr name="base_indicator_margin_left" />
        <attr name="base_indicator_margin_top" />
        <attr name="base_indicator_margin_right" />
        <attr name="base_indicator_margin_bottom" />
        <attr name="base_indicator_corner_radius" />
        <attr name="base_indicator_anim_enable" />
        <attr name="base_indicator_anim_duration" />
        <attr name="base_indicator_bounce_enable" />

        <!-- divider -->
        <attr name="base_divider_color" />
        <attr name="base_divider_width" />
        <attr name="base_divider_padding" />

        <!-- tab -->
        <attr name="base_tab_padding" />
        <attr name="base_tab_space_equal" />
        <attr name="base_tab_width" />

        <!-- title -->
        <attr name="base_titlesize" />
        <attr name="base_textSelectColor" />
        <attr name="base_textUnSelectColor" />
        <attr name="base_textBold" />
        <attr name="base_textAllCaps" />

        <attr name="base_bar_color" format="color" />
        <attr name="base_bar_stroke_color" format="color" />
        <attr name="base_bar_stroke_width" format="dimension" />

    </declare-styleable>

    <declare-styleable name="base_TTabLayout">
        <!-- indicator -->
        <attr name="base_indicator_color" />
        <attr name="base_indicator_height" />
        <attr name="base_indicator_width" />
        <attr name="base_indicator_margin_left" />
        <attr name="base_indicator_margin_top" />
        <attr name="base_indicator_margin_right" />
        <attr name="base_indicator_margin_bottom" />
        <attr name="base_indicator_corner_radius" />
        <attr name="base_indicator_gravity" />
        <attr name="base_indicator_style" />
        <attr name="base_indicator_anim_enable" />
        <attr name="base_indicator_anim_duration" />
        <attr name="base_indicator_bounce_enable" />

        <!-- underline -->
        <attr name="base_underline_color" />
        <attr name="base_underline_height" />
        <attr name="base_underline_gravity" />

        <!-- divider -->
        <attr name="base_divider_color" />
        <attr name="base_divider_width" />
        <attr name="base_divider_padding" />

        <!-- tab -->
        <attr name="base_tab_padding" />
        <attr name="base_tab_space_equal" />
        <attr name="base_tab_width" />

        <!-- title -->
        <attr name="base_titlesize" />
        <attr name="base_textSelectColor" />
        <attr name="base_textUnSelectColor" />
        <attr name="base_textBold" />
        <attr name="base_textAllCaps" />

        <!-- icon -->
        <!-- 设置icon宽度 -->
        <attr name="base_iconWidth" format="dimension" />
        <!-- 设置icon高度 -->
        <attr name="base_iconHeight" format="dimension" />
        <!-- 设置icon是否可见 -->
        <attr name="base_iconVisible" format="boolean" />
        <!-- 设置icon显示位置,对应Gravity中常量值 -->
        <attr name="base_icon_gravity" format="enum">
            <enum name="LEFT" value="3" />
            <enum name="TOP" value="48" />
            <enum name="RIGHT" value="5" />
            <enum name="BOTTOM" value="80" />
        </attr>
        <!-- 设置icon与文字间距 -->
        <attr name="base_iconMargin" format="dimension" />

    </declare-styleable>

    <declare-styleable name="base_RxAutoImageView">
        <attr name="base_ImageSrc" format="reference" />
    </declare-styleable>


    <declare-styleable name="base_SpinKitView">
        <attr name="base_SpinKit_Style">
            <enum name="RotatingPlane" value="0" />
            <enum name="DoubleBounce" value="1" />
            <enum name="Wave" value="2" />
            <enum name="WanderingCubes" value="3" />
            <enum name="Pulse" value="4" />
            <enum name="ChasingDots" value="5" />
            <enum name="ThreeBounce" value="6" />
            <enum name="Circle" value="7" />
            <enum name="CubeGrid" value="8" />
            <enum name="FadingCircle" value="9" />
            <enum name="FoldingCube" value="10" />
            <enum name="RotatingCircle" value="11" />
            <enum name="MultiplePulse" value="12" />
            <enum name="PulseRing" value="13" />
            <enum name="MultiplePulseRing" value="14" />
        </attr>
        <attr name="base_SpinKit_Color" format="color" />
    </declare-styleable>

    <declare-styleable name="base_RxShapeLoadingView">
        <attr name="base_loadingText" format="string" />
        <attr name="base_loadingTextAppearance" format="reference" />
    </declare-styleable>

    <attr name="base_SpinKitViewStyle" format="reference" />
</resources>